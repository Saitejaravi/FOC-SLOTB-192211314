#include <stdio.h>
#include <string.h>

int main() {
    int num = 12345;
    char str[10];
    sprintf(str, "%d", num);
    int len = strlen(str);

    for (int i = 0; i < len / 2; i++) {
        char temp = str[i];
        str[i] = str[len - i - 1];
        str[len - i - 1] = temp;
    }

    int reversedNum = atoi(str);
    printf("%d\n", reversedNum);
    return 0;
}
